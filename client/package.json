    "@types/react-dom": "^16.9.5",
    "apollo-boost": "^0.4.7",
    "apollo-link": "^1.2.14",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.20",
    "class-validator": "^0.12.2",
    "date-fns": "^2.13.0",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
    "immer": "^6.0.9",
    "ismobilejs": "^1.1.1",
    "lerna": "^3.22.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
