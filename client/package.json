{
  "name": "@marblez/client",
  "version": "0.0.0",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@marblez/graphql": "^0.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "apollo-boost": "^0.4.7",
    "date-fns": "^2.13.0",
    "emotion-theming": "^10.0.27",
    "graphql": "^14.6.0",
    "react": "^16.13.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@types/parcel-bundler": "^1.12.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-pwa-manifest": "^3.0.6"
  },
  "alias": {
    "util": "./mocks/utils.js",
    "glob": "./mocks/empty-module.js",
    "fs": "./mocks/empty-module.js",
    "path": "./mocks/empty-module.js"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "pwaManifest": {
    "name": "Marblez",
    "shortName": "Marblez",
    "startURL": "./",
    "theme": "#ff385c",
    "generateIconOptions": {
      "baseIcon": "./src/images/logo.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  }
}
